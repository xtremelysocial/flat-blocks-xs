{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"title": "Default - Source Sans & Source Serif",
	"styles": {
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--source-sans-pro)"
		},
		"blocks": {
			"core/heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--source-serif-pro)"
				}
			},
			"core/site-title": {
				"typography": {
					"fontWeight": "400"
				}
			}
		},
		"elements": {
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--source-serif-pro)"
				}
			}
		}
	}
}